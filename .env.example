# Atlas Admin Portal - environment template
# Copy to .env.local and fill in values. Never commit .env.local.

VITE_API_BASE=https://localhost:5001
VITE_TENANT_SLUG=
VITE_AUTH_DISABLED=false
VITE_AUTH_BYPASS=false
VITE_AUTH0_DOMAIN=your-tenant.auth0.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_CALLBACK_PATH=/auth/callback
VITE_DEFAULT_AFTER_LOGIN=/bookings
VITE_ALLOWED_EMAILS=["admin@example.com"]
# Optional: VITE_USE_MOCK_AVAILABILITY=true | VITE_USE_MOCK_TEMPLATES=true for dev without backend
