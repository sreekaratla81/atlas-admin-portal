# Atlas Admin Portal â€” Environment Variables
# Copy to .env.local and fill in real values. Never commit .env.local.

# API base URL (no trailing slash)
VITE_API_BASE=http://localhost:5120

# Tenant slug sent as X-Tenant-Slug header
VITE_TENANT_SLUG=atlas

# Auth0 configuration (leave empty + set VITE_AUTH_BYPASS=true for local dev)
VITE_AUTH0_DOMAIN=
VITE_AUTH0_CLIENT_ID=
VITE_AUTH0_CALLBACK_PATH=/auth/callback
VITE_DEFAULT_AFTER_LOGIN=/bookings

# Auth bypass for local development (set to true to skip Auth0 login)
VITE_AUTH_DISABLED=false
VITE_AUTH_BYPASS=true

# Comma-separated allowed email addresses (leave empty to allow all)
VITE_ALLOWED_EMAILS=

# Mock availability data (development only)
VITE_USE_MOCK_AVAILABILITY=false
